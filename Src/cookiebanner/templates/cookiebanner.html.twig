        <p>{{ message.information|raw }}</p>
        <h3>{{ message.title|raw }}</h3>
      </div>

      <div class="cookie-banner-categories">
          
          {% if message.cookieactive1 == true %}
            <div class="cookie-banner-category cookie-banner-cookie1">
