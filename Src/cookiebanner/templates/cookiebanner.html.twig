                <label class="switch">
                <input class="input" type="checkbox">
                <span class="slider round"></span>
                </label>
                <label for="">{{message.cookietitle1}}</label>
              </div>
              <div class="cookie-banner-category-description">{{message.cookiedescription1}}</div>
            </div>
